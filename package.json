{
  "name": "Universe",
  "version": "1.1.0",
  "description": "Universe is a dApp solutions community removing blockers that impede growth and adoption.",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/ -o src/styles/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "ajv": "^6.5.4",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.3"
  },
  "devDependencies": {
    "px-rem-reference": "^1.2.2"
  }
}
